# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration (PostgreSQL with Prisma)
DATABASE_URL=postgresql://username:password@localhost:5432/steakz-db?schema=public

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_string_make_it_long_and_random

# Optional: Frontend URL for CORS (if different from defaults)
FRONTEND_URL=http://localhost:5176

# Optional: Node Environment
NODE_ENV=development

# Optional: Server Port
PORT=3001
